#include <stdio.h>
#include <ctype.h>
int main() {
    char ch;
    printf("Enter a character: ");
    scanf(" %c", &ch);
    
    if(isalpha(ch)) {
        printf("Alphabet\n");
        if(isupper(ch)) printf("Uppercase\n");
        else printf("Lowercase\n");
    } else {
        printf("Not alphabet\n");
    }
    
    return 0;
}
