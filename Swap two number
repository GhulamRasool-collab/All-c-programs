#include <stdio.h>
int main() {
    int a, b;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);
    
    printf("Before: a = %d, b = %d\n", a, b);
    
    // Using temporary variable
    int temp = a;
    a = b;
    b = temp;
    
    printf("After: a = %d, b = %d\n", a, b);
    return 0;
}
