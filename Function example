#include <stdio.h>

// Function declaration
int add(int a, int b);

int main() {
    int x, y, sum;
    
    printf("Enter two numbers: ");
    scanf("%d %d", &x, &y);
    
    sum = add(x, y);
    printf("Sum: %d\n", sum);
    
    return 0;
}

// Function definition
int add(int a, int b) {
    return a + b;
}
